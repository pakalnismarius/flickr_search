<% if @list.any? %>
  <% @list.each do |row| %>
    <div class="row mt-40">
      <% row.each_with_index do |url, index| %>
        <div class="col-md-3" align="center">
          <%= image_tag(row[index]) %>
        </div>
      <% end %>
    </div>
  <% end %>
<% else %>
  <h4 align="center">Can't find anyting, try again!</h4>
<% end %>
